{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "outDir": "./dist",
    "noEmit": false,
    "moduleResolution": "node"
  },
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "node_modules"
  ]
}